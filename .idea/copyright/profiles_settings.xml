<component name="CopyrightManager">
  <settings default="MIT">
    <module2copyright>
      <element module="include" copyright="MIT" />
      <element module="src" copyright="MIT" />
      <element module="test" copyright="MIT" />
    </module2copyright>
    <LanguageOptions name="C/C++">
      <option name="fileTypeOverride" value="3" />
      <option name="separateBefore" value="true" />
      <option name="lenBefore" value="2" />
    </LanguageOptions>
  </settings>
</component>